{
  "dashboard": {
    "id": null,
    "title": "Neural Cryptanalysis - Overview",
    "tags": ["neural-cryptanalysis", "overview"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "hideControls": false,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "schemaVersion": 36,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"neural-crypto-api\"}",
            "refId": "A",
            "legendFormat": "API Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(neural_crypto_requests_total[5m])",
            "refId": "A",
            "legendFormat": "Requests/sec"
          }
        ],
        "yAxes": [
          {
            "label": "requests/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(neural_crypto_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(neural_crypto_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(neural_crypto_request_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "seconds",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(neural_crypto_requests_total{status=~\"4..\"}[5m])",
            "refId": "A",
            "legendFormat": "4xx errors"
          },
          {
            "expr": "rate(neural_crypto_requests_total{status=~\"5..\"}[5m])",
            "refId": "B",
            "legendFormat": "5xx errors"
          }
        ],
        "yAxes": [
          {
            "label": "errors/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Active Experiments",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_crypto_active_experiments",
            "refId": "A",
            "legendFormat": "Active Experiments"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Model Training Jobs",
        "type": "stat",
        "targets": [
          {
            "expr": "neural_crypto_active_training_jobs",
            "refId": "A",
            "legendFormat": "Training Jobs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16}
      },
      {
        "id": 7,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container=\"neural-crypto-api\"}[5m]) * 100",
            "refId": "A",
            "legendFormat": "CPU Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "percent",
            "min": 0,
            "max": 100
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(container_memory_usage_bytes{container=\"neural-crypto-api\"} / container_spec_memory_limit_bytes{container=\"neural-crypto-api\"}) * 100",
            "refId": "A",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yAxes": [
          {
            "label": "percent",
            "min": 0,
            "max": 100
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16}
      },
      {
        "id": 9,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends",
            "refId": "A",
            "legendFormat": "Active Connections"
          }
        ],
        "yAxes": [
          {
            "label": "connections",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "Redis Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "refId": "A",
            "legendFormat": "Commands/sec"
          }
        ],
        "yAxes": [
          {
            "label": "ops/sec",
            "min": 0
          },
          {
            "show": false
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20}
      }
    ]
  }
}